<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!--<ProjectTypeGuids>{10368E6C-D01B-4462-8E8B-01FC667A7035};{FAE04EC0-301F-11D3-BF4B-00C04F79EFBC}</ProjectTypeGuids>-->
    <!--VS project type is a function of the first ProjectTypeGuids found in the text of the project file-->
  </PropertyGroup>
  <!--
  
    Properties
  -->
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildProjectDirectory)\, .pre.props))\.pre.props" />
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), .props))\.props" />
  <!--
  
    Hierarchy
  -->
  <ItemGroup>
    <!--all E { monodroid.bindings }-->
    <PlatformGroup Include="$(AllPlatformId)">
      <Platforms>$(AndroidBindingsLibraryPlatformId)</Platforms>
    </PlatformGroup>
    <!--monodroid.bindings E { monodroid.bindings-4.0.3 }-->
    <PlatformGroup Include="$(AndroidBindingsLibraryPlatformId)">
      <Platforms>$(AndroidBindingsLibraryPlatformId-4_0_3)</Platforms>
    </PlatformGroup>
  </ItemGroup>
  <!--
  
    Universal
  -->
  <PropertyGroup Condition=" '$(Platform)' == '[none]' " />
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <ProjectGuid>{3B72465B-ACAE-43AE-9327-10F372FE5F80}</ProjectGuid>
    <AssemblyName>FormsViewGroup</AssemblyName>
    <RootNamespace>FormsViewGroup</RootNamespace>
  </PropertyGroup>
  <ImportGroup>
    <Import Project="nuspec.props" />
  </ImportGroup>
  <ItemGroup>
    <None Include="nuspec.props" />
  </ItemGroup>
  <!--
  
    Library
  -->
  <ItemGroup Condition="'$(IsLibraryPlatform)'=='true'">
    <Compile Include="$(SrcDir)\GlobalAssemblyInfo.cs">
      <Link>Properties\GlobalAssemblyInfo.cs</Link>
      <IsMxbuildTemplate>true</IsMxbuildTemplate>
    </Compile>
    <Compile Include="$(ExtIdDir)BuildInfo.cs">
      <DependentUpon>BuildInfo.t.cs</DependentUpon>
      <Link>Properties\BuildInfo.cs</Link>
    </Compile>
    <None Include="$(ExtIdDir)BuildInfo.t.cs">
      <IsMxbuildTemplate>true</IsMxbuildTemplate>
      <Link>Properties\BuildInfo.t.cs</Link>
      <CompileOutput>BuildInfo.cs</CompileOutput>
    </None>
    <Compile Include="$(ExtPackDir)PackageInfo.cs">
      <Link>Properties\PackageInfo.cs</Link>
    </Compile>
  </ItemGroup>
  <!--
  
    Android Bindings
  -->
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'debug|monoDroid.bindings' " />
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'release|monoDroid.bindings' " />
  <PropertyGroup Condition=" '$(MetaPlatform)' == '$(AndroidBindingsLibraryPlatformId)' ">
    <AndroidCodeGenTarget>XAJavaInterop1</AndroidCodeGenTarget>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <AndroidUseLatestPlatformSdk>True</AndroidUseLatestPlatformSdk>
    <TargetFrameworkVersion>v7.0</TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(MetaPlatformIdentifier)' == '$(AndroidBindingsLibraryPlatformId)' ">
    <EmbeddedJar Include="Jars\formsviewgroup.jar" />
    <TransformFile Include="Transforms\Metadata.xml" />
    <!--<Compile Include="Properties\AssemblyInfo.cs" />-->
    <!--references-->
    <Reference Include="Mono.Android" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
  </ItemGroup>
  <!--

    Targets
  -->
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), .targets))\.targets" />
</Project>